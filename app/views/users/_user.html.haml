%tr
  %td
    = user.id
  %td
    = user.email
  %td
    - if user.last_sign_in_at
      = user.last_sign_in_at.to_formatted_s(:short)
    - else
      "Not Yet"
  %td
    = link_to user_path(id: user.id) , method: :delete,  data: { confirm: "You are sure ?" }  do
      %span.glyphicon.glyphicon-remove(aria-hidden=true)